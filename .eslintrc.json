{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["node", "prettier"],
    "rules": {
      "node/no-unsupported-features/es-syntax": "off",
      "node/no-unpublished-require": "off",
      "prettier/prettier": "error"
    }
  }
